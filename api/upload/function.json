{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "dataType": "binary",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "storage",
      "type": "blob",
      "path": "images/{filename}",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
  }
  ],
  "scriptFile": "../dist/upload/index.js"
}